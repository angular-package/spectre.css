extends ../_layout/_docs-layout.pug

block variables
  - var slug = 'hero'
  - var name = 'Hero'
  - var parent = 'layout'
  - var title = 'Hero - Layout - Spectre.css CSS Framework'
  - var description = 'The Hero container shows key featured content or information. Spectre.css CSS Framework is a lightweight, responsive and modern CSS framework for faster and extensible development.'

block docs-content
  +docs-heading(slug, name, { parent, name: 'Layout' })
    +docs-menu(name, {parent, slug},
      {
        anchor: 'sizes',
        name: 'Sizes',
        items: [
          { name: 'Available', anchor: 'available' },
          { name: 'Extra small', anchor: 'xs' },
          { name: 'Small', anchor: 'sm' },
          { name: 'Medium', anchor: 'md' },
          { name: 'Large', anchor: 'lg' },
          { name: 'Extra large', anchor: 'xl' }
        ]
      },
      {
        anchor: 'colors',
        name: 'Colors',
        items: [
          { name: 'Available', anchor: 'available' },
          { name: 'Dark', anchor: 'dark' },
          { name: 'Light', anchor: 'light' },
          { name: 'Accent', anchor: 'accent' },
          { name: 'Primary', anchor: 'primary' },
          { name: 'Secondary', anchor: 'secondary' },
          { name: 'Gray', anchor: 'gray' },
          { name: 'Bg', anchor: 'bg' },
          { name: 'Border', anchor: 'border' },
          { name: 'Link', anchor: 'link' },
          { name: 'Control', anchor: 'control' },
          { name: 'Body', anchor: 'body' },
          { name: 'Other', anchor: 'other' }
        ]
      }
    )
    include ../_layout/_ad-g.pug

    p
      | The #[strong hero] container shows key featured content or information.
    p
      a.btn.btn-accent(href='https://angular-package.dev/ui-kit/component/hero', target='_blank') Demonstration

    .docs-demo.columns
      .tab-container.column.col-12
        ul.tab
          li.tab-item.active
            a(onclick="") Preview
          li.tab-item
            a() Source
        .tab-content
          .preview
            .hero.hero-bg.text-gray
              .hero-body
                h1 Hero title
                p This is a hero example

          .source
            pre.code(data-lang='HTML')
              code
                :highlight(lang="html")
                  <div class="hero hero-bg text-gray">
                    <div class="hero-body">
                      <h1>Hero title</h1>
                      <p>This is a hero example</p>
                    </div>
                  </div>

    .docs-demo.columns
      .column.col-12
        .hero.hero-bg.text-gray
          .hero-body
            h1 Hero prefixed title
            p This is a hero prefixed example
    pre.code(data-lang='HTML')
      code
        :highlight(lang="html")
          <!-- Prefixed -->
          <div class=".hero hero-gray text-gray">
            <div class="hero-body">
              <h1>Hero prefixed title</h1>
              <p>This is a hero prefixed example</p>
            </div>
          </div>

    +docs-subheading('sizes-available', 'Hero sizes', {slug})

    p
      | There are following hero sizes available to use. Each color has its own size from #[code xs] to #[code xl] and to use it just add color name before the size.
      | For example color primary has its own size in the class #[code .hero-primary-size-xs] to #[code .hero-primary-size-xl].

    .docs-demo.columns
      .column.col-12
        table.docs-table.table.table-striped.table-hover.text-center
          thead
            tr
              th
              th xs 
              th sm 
              th md  
              th lg  
              th xl 
          tbody
            tr
              th.text-left Available
              td
                +docs-dot(true)
              td
                +docs-dot(true)
              td
                +docs-dot(true)
              td
                +docs-dot(true)
              td
                +docs-dot(true)
            tr
              th.text-left Size
              td #[code 1rem]
              td #[code 2rem]
              td #[code 4rem]
              td #[code 8rem]
              td #[code 10rem]
            tr
              th.text-left CSS variable
              td #[code --s-hero-size-xs]
              td #[code --s-hero-size-sm]
              td #[code --s-hero-size-md]
              td #[code --s-hero-size-lg]
              td #[code --s-hero-size-xl]
    p
      | Use sizes from the 
      code hero-size-xs
      |  to 
      code hero-size-xl
      |  to the hero container for the smallest to the largest padding. And 
      code hero-size-vh
      |  as view height #[code 100%].

    .docs-demo.columns
      .column.col-12
        +docs-subheading('sizes-xs', 'Size extra small', { slug, tag: 'h5' })
        .hero.hero-size-xs.hero-primary
          .hero-body
            h1 Hero extra small
            p This is a hero example size extra small
    pre.code(data-lang='HTML')
      code
        :highlight(lang="html")
